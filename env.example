# FutPrint API Configuration
# Copy this file to .env and fill in your actual values

# API Base URL for OHLCV data
API_OHLCV_URL=https://api.futprint.in/api/historical-data-ohlcv-oi

# Your FutPrint API Bearer Token
API_TOKEN=your_bearer_token_here

# Admin API Key (typically 'FutPrintIN')
API_KEY=FutPrintIN

# Risk-free rate for options pricing (default: 6%)
RISK_FREE_RATE=0.06

# Default lot size for NIFTY contracts
DEFAULT_LOT_SIZE=75

# Trading parameters
DEFAULT_FEE_PER_CONTRACT=10.0
DEFAULT_SLIPPAGE_TICKS=1
DEFAULT_TICK_SIZE=1.0
